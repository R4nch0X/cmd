{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "beberlei/assert": "^1.2",
        "beberlei/doctrineextensions": "^1.3",
        "cakephp/chronos": "^2.3",
        "elfsundae/laravel-hashid": "^1.7",
        "gedmo/doctrine-extensions": "^3.7",
        "guzzlehttp/guzzle": "^7.4",
        "itsgoingd/clockwork": "^5.1",
        "laravel-doctrine/extensions": "^1.5",
        "laravel-doctrine/fluent": "^1.1",
        "laravel-doctrine/orm": "^1.8",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "spatie/laravel-ignition": "^1.3"
    },
    "require-dev": {
        "arcanedev/log-viewer": "^9.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-dump-server": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.8",
        "garygreen/pretty-routes": "^1.0",
        "illuminate/support": "^9.22",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/larastan": "^2.1",
        "nunomaduro/phpinsights": "^2.4",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
