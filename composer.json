{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "beberlei/assert": "^1.7",
    "beberlei/doctrineextensions": "^1.3",
    "cakephp/chronos": "^2.3",
    "elfsundae/laravel-hashid": "^1.7",
    "flugger/laravel-responder": "^3.2",
    "gedmo/doctrine-extensions": "^3.10",
    "guzzlehttp/guzzle": "^7.5",
    "itsgoingd/clockwork": "^5.1",
    "laravel-doctrine/extensions": "^1.5.1",
    "laravel-doctrine/fluent": "^1.3.1",
    "laravel-doctrine/orm": "^1.8",
    "laravel/framework": "^9.40",
    "laravel/sanctum": "^2.15.1",
    "laravel/tinker": "^2.7",
    "ramsey/uuid-doctrine": "^1.8",
    "spatie/laravel-ignition": "^1.6"
  },
  "require-dev": {
    "arcanedev/log-viewer": "^9.0",
    "barryvdh/laravel-debugbar": "^3.7",
    "barryvdh/laravel-ide-helper": "^2.12",
    "beyondcode/laravel-dump-server": "^1.8",
    "friendsofphp/php-cs-fixer": "^3.13",
    "garygreen/pretty-routes": "^1.0",
    "illuminate/support": "^9.22",
    "laravel/sail": "^1.16",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^6.3",
    "nunomaduro/larastan": "^2.2",
    "nunomaduro/phpinsights": "^2.6",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-faker": "^1.0",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "pestphp/pest-plugin-laravel": "^1.2",
    "spatie/laravel-ignition": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Cmd\\": "src/",
      "Database\\Factories\\": "Cmd/Entities/Factories",
      "DatabaseSeeder\\": "app/Development/Seeders"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "build": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "php artisan key:generate --ansi",
      "php artisan vendor:publish --tag=\"assets\"",
      "php artisan doctrine:clear:metadata:cache",
      "php artisan doctrine:generate:proxies",
      "php artisan doctrine:schema:update --force",
      "php artisan migrate"
    ],
    "ide-helper:generate": "@php artisan ide-helper:generate -M -H",
    "ide-helper:meta": "@php artisan ide-helper:meta",
    "ide-helper:models": "@php artisan ide-helper:models -N",
    "insights": "@php artisan insights -s",
    "pint": "./vendor/bin/pint",
    "stan": "./vendor/bin/phpstan analyse --memory-limit=2G",
    "test": "@php artisan test"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
